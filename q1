import java.util.Scanner;
public class Q1 {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int var;
        System.out.println("Entar a Number");
        var = scan.nextInt();
        var %= 2;
        if (var == 0){
            System.out.println("E");
        }
         else{
             System.out.println("O");
         }  
        
        
    }
}
